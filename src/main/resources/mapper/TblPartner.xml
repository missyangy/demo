<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cn.demo.dao.TblPartnerMapper">


    <select id="queryByValid" resultType="com.cn.demo.model.Partner">
        SELECT
        *
        FROM
        tbl_partner a
        where a.valid=#{valid}
    </select>


</mapper>